{
  "name": "CodeRunner",
  "version": "1.0.0",
  "description": "A Code Runner Extension For Fast Learning",
  "manifest_version": 3,

  "permissions": ["clipboardRead","clipboardWrite"],
  "background": {
    "service_worker ": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./Icons/32.png",
    "32": "./Icons/32.png",
    "48": "./Icons/32.png"
  }
}
